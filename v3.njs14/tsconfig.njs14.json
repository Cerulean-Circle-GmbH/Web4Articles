{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "target": "ES2015",
    "module": "ES2015",
    "moduleResolution": "node16",
    "outDir": "../dist.njs14",
    "declaration": false,
    "sourceMap": false,
    "noEmit": false,
    "allowImportingTsExtensions": true,
    "skipLibCheck": true
  },
  "include": [
    "../src/**/*.ts",
    "../v2/src/**/*.ts"
  ],
  "exclude": [
    "../node_modules",
    "../dist",
    "../dist.njs14"
  ]
}